// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
    blockHash           String
    blockNumber         String
    from                String
    gas                 String
    gasPrice            String
    hash                String
    input               String
    nonce               String
    to                  String
    transactionIndex    String  @unique
    value               String
    type                String
    v                   String
    r                   String
    s                   String
}
